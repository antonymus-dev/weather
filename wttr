import requests


url = 'https://wttr.in/London?mMnqT&lang=ru'
response = requests.get(url)
response.raise_for_status()
print(response.text)

url = 'https://wttr.in/SVO?mMnqT&lang=ru'
response = requests.get(url)
response.raise_for_status()
print(response.text)

url = 'https://wttr.in/Череповец?mMnqT&lang=ru'
response = requests.get(url)
response.raise_for_status()
print(response.text)
